{
  "name": "<%= projectName %>",
  "version": "0.0.0",
  "description": "<%= projectDescription %>",
  "author": {
    "name": "<%= projectAuthorName %>",
    "email": "<%= projectAuthorEmail %>",
    "url": "https://github.com/Jam3"
  },
  "scripts": {
    "install": "npm run start",
    "start": "npm run copy && npm run style && npm run dev",
    "copy": "node scripts/copy.js",
    "style": "node scripts/style.js",
    "dev": "node scripts/dev.js",
    "release": "npm run release-style && npm run release-browserify && npm run release-copy && npm run release-gzip",
    "release-style": "node scripts/style.js --env=production",
    "release-browserify": "node scripts/release.js --env=production",
    "release-copy": "node scripts/copy.js --env=production",
    "release-gzip": "node scripts/gzip.js --env=production"
  },
  "license": "ISC",
  "repository": "<%= projectRepository %>",
  "dependencies": {
    "bigwheel": "^2.2.0",
    "domready": "^1.0.8",
    "bw-analytics": "^1.1.1",
    "gsap": "^1.15.0"<% if (templateLibrary === 'vue' ) { %>,
    "vue": "^1.0.0-migration"<% } else if (templateLibrary === 'hbs' ) { %>,
    "handlebars": "^2.0.0",
    "domify": "^1.4.0"<% } else if (templateLibrary === 'pixi-ears') { %>,
    "pixi": "^0.2.1",
    "pixi-ears": "^1.0.0"<% } %>
  },
  "devDependencies": {
    "budo": "^6.1.0",
    "concat-stream": "^1.5.1",
    "envify": "^3.4.0",
    "garnish": "^4.1.1",
    "glob": "^6.0.1",<% if (css === 'less') { %>
    "less": "^2.5.3",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-glob": "^1.1.1",<% } else if (css === 'scss') { %>
    "autoprefixer": "^6.2.3",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.14",
    "sass-glob-importer": "^1.0.1",<% } %>
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "pngquant-bin": "^3.0.0",
    "uglify-js": "^2.6.1",<% if (templateLibrary!=='none') { %>
    "brfs": "^1.2.0",
    "browserify-shim": "^3.8.0",<% } %>
    "browserify": "^12.0.1",
    "envify": "^3.4.0"<% if (useES6) { %>,
    "babelify": "^6.1.3"<% } %>
  },
  "browserify": {
    "transform": [
      "envify"<% if (templateLibrary!=='none') { %>,
      "browserify-shim",
      "brfs"<% } %><% if (useES6) { %>,
      "babelify"<% } %>
    ]
  },
  <% if (templateLibrary!=='none') { %>
  "browserify-shim": {

  },
  <% } %>
  "browser": {}
}
