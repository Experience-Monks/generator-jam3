{
  "name": "{{repoName}}",
  "version": "0.0.0",
  "description": "{{description}}",{{#is app 'electron'}}
  "main": "app/electron.index.js", {{/is}}
  "author": {
    "name": "{{author}}",
    "email": "{{email}}",
    "url": "https://github.com/Jam3"
  },
  "scripts": { {{#is app 'electron'}}
    "start": "electron ./scripts/electron.dev.js --env=development",{{/is}}{{#is app 'no'}}
    "start": "npm run copy && npm run style && npm run dev", {{/is}}
    "copy": "node scripts/copy.js",
    "style": "node scripts/style.js",{{#is app 'no'}}
    "dev": "node scripts/dev.js", {{/is}} {{#is app 'chrome'}} 
    "dev": "node scripts/chrome.dev.js", {{/is}} 
    "release": "npm run release-clean && npm run release-style && npm run release-browserify && npm run release-copy && npm run release-gzip",
    "release-style": "node scripts/style.js --env=production",
    "release-browserify": "node scripts/release.js --env=production",
    "release-copy": "node scripts/copy.js --env=production",
    "release-gzip": "node scripts/gzip.js --env=production",
    "release-clean": "node scripts/clean.js --env=production", {{#is app 'electron'}}
    "package": "npm run release && node scripts/electron.package.js --env=production",
    "package-all": "npm run package -- --all", {{/is}}
    "lowercase": "node scripts/lowercase.js"
  },
  "license": "ISC",
  "repository": "{{repo}}",
  "dependencies": { {{#is framework 'bigwheel'}}
    "bigwheel": "^3.0.0",
    "domready": "^1.0.8",
    "bw-analytics": "^1.1.1",
    "gsap": "^1.18.2",
    "handlebars": "^2.0.0",
    "domify": "^1.4.0"{{/is}}{{#is framework 'react'}}
    "babel-polyfill": "^6.7.4",
    "domready": "^1.0.8",
    "glob": "^6.0.4",
    "node-sass-glob": "^1.0.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-f1": "^7.0.0",
    "react-redux": "^4.4.4",
    "react-router": "^2.1.1",
    "react-router-redux": "^4.0.2",
    "react-transition-group-plus": "^0.1.5",
    "redux": "^3.4.0",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.9"{{/is}}
  },
  "devDependencies": {
    "budo": "^8.0.4",
    "rimraf": "^2.5.2",
    "envify": "^3.4.0",
    "concat-stream": "^1.5.1",
    "garnish": "^4.1.1",
    "graceful-fs": "^4.1.3",
    "handlebars": "^4.0.5",
    "glob": "^6.0.1",{{#is css 'less'}}
    "less": "^2.5.3",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-glob": "^1.1.1",{{/is}}{{#is css 'scss'}}
    "autoprefixer": "^6.2.3",
    "node-sass": "^3.4.2",
    "postcss": "^5.0.14",
    "node-sass-glob": "^1.0.4",{{/is}}
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "pngquant-bin": "^3.0.0",
    "uglify-js": "^2.6.1",{{#is framework 'bigwheel'}}
    "brfs": "^1.2.0",
    "browserify-shim": "^3.8.0",{{/is}}{{#is app 'electron'}}
    "electron-connect": "^0.4.2",
    "electron-debug": "^1.0.1",
    "electron-devtools-installer": "^2.0.0",
    "electron-packager": "^7.4.0",
    "electron-prebuilt": "^1.3.0",
    "os": "^0.1.1",
    "fs-extra": "^0.30.0",{{/is}}
    "browserify": "^12.0.1"{{#if useES6}},
    "babelify": "^7.2.0",
    "babel-runtime": "^5.8.34",
    "babel-preset-es2015": "^6.3.13"{{#is framework 'react'}},
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0"{{/is}}{{/if}}
  },
  "browserify": {
    "transform": [{{#is framework 'bigwheel'}}
      "browserify-shim",
      "brfs",{{/is}}{{#if useES6}}
      "babelify",{{/if}}
      "envify"
    ]
  }{{#is framework 'bigwheel'}},
  "browserify-shim": {

  },
  "browser": {}{{/is}}
}
