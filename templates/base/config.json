{
  "defaults": {
    "entry": "./src/index.js",
    "vendor": {{#if vendor}}"vendor.js"{{else}}false{{/if}},
    "bundle": "bundle.js",
    "static": "./static/",
    "raw": "./raw-assets/",
    "style": "./src/style/main.{{css}}",
    "ASSET_PATH": "./assets/",
    "BASENAME": "/",
    "JPEG_QUALITY": 80,
    "templateBlacklist": [],
    "htpasswd": {{#if password}}"{{passLocation}}"{{else}}false{{/if}},
    "env": {}
  },
  "development": {
    "output": "./build/"
  },
  "production": {
    "timestamp": true,
    "output": "./release/",
    "minify": true,
    "removeLogs": true
  }
}

